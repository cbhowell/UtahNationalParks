<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <Button layoutX="551.0" layoutY="26.0" mnemonicParsing="false" onMouseClicked="#submit" text="SUBMIT" textFill="#08b562">
      <font>
         <Font size="17.0" />
      </font>
      <effect>
         <Reflection />
      </effect>
   </Button>
   <Button layoutX="664.0" layoutY="26.0" mnemonicParsing="false" onMouseClicked="#reset" text="RESET" textFill="#b90303">
      <font>
         <Font size="17.0" />
      </font>
      <effect>
         <Reflection />
      </effect>
   </Button>
    <MenuButton fx:id="startMenuBTN" layoutX="24.0" layoutY="26.0" mnemonicParsing="false" text="Start Location" textAlignment="CENTER">
      <items>
          <MenuItem mnemonicParsing="false" onAction="#startArchesSelected" text="Arches National Park" />
          <MenuItem mnemonicParsing="false" onAction="#startBryceSelected" text="Bryce National Park" />
          <MenuItem mnemonicParsing="false" onAction="#startCanyonLandsSelected" text="Canyonlands National Park" />
          <MenuItem mnemonicParsing="false" onAction="#startCapitolReefSelected" text="Capitol Reef National Park" />
          <MenuItem mnemonicParsing="false" onAction="#startZionSelected" text="Zion National Park" />
      </items>
        <font>
          <Font size="17.0" />
        </font>
      <effect>
         <Reflection />
      </effect>
    </MenuButton>
    <MenuButton fx:id="destMenuBTN" layoutX="299.0" layoutY="26.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="146.0" text="Destination" textAlignment="CENTER">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#destArchesSelected" text="Arches National Park" />
          <MenuItem mnemonicParsing="false" onAction="#destBryceSelected" text="Bryce National Park" />
          <MenuItem mnemonicParsing="false" onAction="#destCanyonLandsSelected" text="Canyonlands National Park" />
          <MenuItem mnemonicParsing="false" onAction="#destCapitolReefSelected" text="Capitol Reef National Park" />
          <MenuItem mnemonicParsing="false" onAction="#destZionSelected" text="Zion National Park" />
        </items>
        <font>
          <Font size="17.0" />
        </font>
      <effect>
         <Reflection />
      </effect>
    </MenuButton>
</AnchorPane>
